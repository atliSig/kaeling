extends ../layouts/default

block content
	.container-fluid
		.row
			.col-12.table-column
				table.table.table-hover
					thead
						tr
							th(scope='col') Company
							th(scope='col') Deadline
							th(scope='col') Job done
					tbody
						each job in jobs
							tr
								td=job.customerObject.name
								td=job.prettyDate
								td=job.createdAt